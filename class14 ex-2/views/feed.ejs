<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }
    html,
    body {
      height: 100%;
      width: 100%;
      background-color: gray;
    }
    .cards {
        display:flex;
        flex-wrap: wrap;
        gap:10px;
        align-items: center;
        justify-content: center;
        padding: 10px;
    }
    .card {
      height: 250px;
      width: 200px;
      background-color: white;
      padding:10px;
      text-align: center;
      border-radius: 10px;
    }
    .card img{
        height: 80px;
        width: 80px;
        border-radius: 50%;
        margin-bottom: 15px;
        object-fit: cover;
        object-position: center;
    }
    h5{
      color: gray;
      margin-top: 5px;
      
    }
  </style>
  <body>
    <main>
      <div class="cards">
        <% user.forEach((elem)=>{ %>
        <div class="card">
          <img src=<%=elem.imageurl%> alt="" />
          <h3><%=elem.username%></h3>
          <h5><%=elem.email%></h5>
        </div>
        <%})%>
      </div>
    </main>
  </body>
</html>
